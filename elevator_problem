#include <stdio.h>
#include <stdlib.h>
int main() {
    int a,b,c;
scanf("%d",&a);
scanf("%d",&b);
scanf("%d",&c);
if(a>0&&b>0&&c>0){
if(abs(a-1)<(abs(b-c)+abs(c-1))){
    printf("1");
}
else{
    if(abs(a-1)>(abs(b-c)+abs(c-1))){
    printf("2");
}
else{
    if(abs(a-1)==(abs(b-c)+abs(c-1))){
    printf("3");
}}}
}
else{
    printf("enter valid no.!!");
}
}
